<?xml version="1.0" encoding="UTF-8"?>

<manifest>
    <!-- Remotes -->
    <remote name="Toki-hub"
        fetch="https://github.com/Kurumi-Tokito/" />
    <remote name="Toki-lab"
        fetch="https://gitlab.com/Kurumi-Tokito/" />

    <!-- Device -->
    <project path="device/xiaomi/munch" name="device_xiaomi_munch"
        remote="Toki-hub" revision="voltage-15" />

    <!-- Vendor -->
    <project path="vendor/xiaomi/munch" name="vendor_xiaomi_munch"
        remote="Toki-lab" revision="voltage-15" />

    <!-- Kernel -->
    <project path="kernel/xiaomi/sm8250" name="nova_kernel_xiaomi_sm8250"
        remote="Toki-hub" revision="voltage-15" />

    <!-- Lineage -->
    <remote name="lineage"
        fetch="https://github.com/LineageOS/" />
    <project path="hardware/xiaomi" name="android_hardware_xiaomi"
        remote="lineage" revision="lineage-22.0" />

    <!-- Graphene Apps -->
    <remote name="grapgene"
        fetch="https://github.com/GrapheneOS/" />
    <project path="external/AppStore" name="platform_external_AppStore"
        remote="grapgene" revision="15" />

    <!-- Kprofiles -->
    <remote name="kprofiles"
        fetch="https://github.com/KProfiles/" />
    <project path="packages/apps/KProfiles" name="android_packages_apps_Kprofiles"
        remote="kprofiles" revision="main" />

    <!-- Remove -->
    <remove-project name="vendor_voltage-priv_keys" />
</manifest>
